#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
    int t;
    int a,b,c;
    cin>>t;
    while(t--)
    {
    cin>>a>>b>>c;
    int x=min(a,b);
    int y=min(b,c);
    int z=min(a,c);
    if(x==y && y==z)
    cout<<"YES"<<endl;
    else
    cout<<"NO"<<endl;
    }
}
